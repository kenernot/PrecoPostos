program ConsutaPrecosPostos;

uses
  System.StartUpCopy,
  FMX.Forms,
  uMain in 'uMain.pas' {FMain},
  uLogin in 'uLogin.pas' {FLogin},
  uCadastroUsuario in 'uCadastroUsuario.pas' {FCadastroUsuario};

